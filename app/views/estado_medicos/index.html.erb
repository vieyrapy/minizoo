<p id="notice"><%= notice %></p>

<h1>Estado Medicos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estado_medicos.each do |estado_medico| %>
      <tr>
        <td><%= estado_medico.nombre %></td>
        <td><%= link_to 'Show', estado_medico %></td>
        <td><%= link_to 'Edit', edit_estado_medico_path(estado_medico) %></td>
        <td><%= link_to 'Destroy', estado_medico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estado Medico', new_estado_medico_path %>
